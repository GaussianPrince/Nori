
struct Out
{
  float4 position : POSITION;
  float2 mapping;
  float2 clipOverF;
};

Out main(uniform float4x4 P,
         in float2 position,
         in float2 mapping,
         in float2 clipOverF)
{
  Out result;
  result.position = mul(P, float4(position, 0, 1));
  result.mapping = mapping;
  result.clipOverF = clipOverF;
  return result;
}

