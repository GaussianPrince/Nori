
#define NORI_VERSION "@VERSION@"

/* Define this to 1 if word order is big-endian */
#cmakedefine NORI_WORDS_BIGENDIAN 1

/* Define this to 1 if building for Win32 */
#cmakedefine NORI_SYSTEM_WIN32 1
/* Define this to 1 if building for Mac OS X */
#cmakedefine NORI_SYSTEM_MACOSX 1
/* Define this to 1 if building for Linux */
#cmakedefine NORI_SYSTEM_LINUX 1

/* Define this to 1 if sys/stat.h is available */
#cmakedefine NORI_HAVE_SYS_STAT_H 1
/* Define this to 1 if fcntl.h is available */
#cmakedefine NORI_HAVE_FCNTL_H 1
/* Define this to 1 if unistd.h is available */
#cmakedefine NORI_HAVE_UNISTD_H 1
/* Define this to 1 if dirent.h is available */
#cmakedefine NORI_HAVE_DIRENT_H 1

/* Define this to 1 if io.h is available */
#cmakedefine NORI_HAVE_IO_H 1
/* Define this to 1 if direct.h is available */
#cmakedefine NORI_HAVE_DIRECT_H 1
/* Define this to 1 if windows.h is available */
#cmakedefine NORI_HAVE_WINDOWS_H 1

/* Define this to 1 to include the networking API */
#cmakedefine NORI_INCLUDE_NETWORK 1
/* Define this to 1 to include the renderers */
#cmakedefine NORI_INCLUDE_RENDERER 1
/* Define this to 1 to include the audio wrapper */
#cmakedefine NORI_INCLUDE_AUDIO 1
/* Define this to 1 to include the UI system */
#cmakedefine NORI_INCLUDE_UI_SYSTEM 1
/* Define this to 1 to include the debug interface */
#cmakedefine NORI_INCLUDE_DEBUG_UI 1
/* Define this to 1 to include the Squirrel bindings */
#cmakedefine NORI_INCLUDE_SQUIRREL 1
/* Define this to 1 to include the Bullet library */
#cmakedefine NORI_INCLUDE_BULLET 1

