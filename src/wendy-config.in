
prefix=@prefix@
exec_prefix=@prefix@

while test $# -gt 0; do

  case $1 in
    --prefix)
      echo $prefix
      ;;
    --cxxflags)
      echo "-I@includedir@ @CXXFLAGS@"
      ;;
    --libs)
      echo "-L@libdir@ -lwendy @LIBS@"
      ;;
    *)
      echo "Punt" 1>&2
      ;;
  esac

  shift
done

